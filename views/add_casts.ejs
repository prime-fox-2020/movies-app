    crossorigin="anonymous"></script>
</head>
<body>
    <h1> Register Cast </h1>
    <form action="/casts/add" method="post">
        <labe>First Name:</labe>
        <br>
        <input type="text" name ="first_name" placeholder="e.g. Naomi">
        <br>
        <label>Last Name:</label>
        <br>
        <input type="text" name="last_name" placeholder="e.g. Scott">
        <br>
        <label> Phone Number:</label>
        <br>
        <input type="text" name="phone_number" placeholder="08768572636">
        <br>
        <label>Birth Year:</label>
        <br>
        <input type="number" name="birth_year" placeholder="1993">
        <br>
        <label>Gender :</label>
        <br>
        <select name ="gender">
            <option value="Male">Male</option>
            <option value="Female">Female</option>
        </select>
    <br>
    <br>
    <button type="submit">Register</button>
    </form>
    <br>
    <a href ="/casts"><button>Back</button></a>
</body>