<!DOCTYPE html>
<html lang=eng>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width , initial-scale=1.0">
        <title>List Movies</title>

    </head>
    <body>
      <h1>Casts</h1>
      <a href="/cs/add"><button>Add Actor / actress</button></a>
      <table>
        <tr>
          <th>Name</th>
          <th>Phone Number</th>
          <th>Birth Year</th>
          <th>Gender</th>
          <th>Action</th>
      </tr>
          <% data.forEach(el=>{ %>
          <tr>
              <td><%=el.first_name %> <%=el.last_name%></td>
              <td><%=el.phone_number%></td>
              <td><%=el.birth_year%></td>
              <td><%=el.gender%></td>
              <td >
                  <a href="/cs/<%=el.id%>/edit"><button type="submit"> Edit </button></a>
                  <a href="/cs/<%=el.id%>/delete"><button type="submit" > Delete </button></a>
                  <a href="/cs/<%=el.id%>/seemovie"><button type="submit"> See Movies</button></a>
              </td>
          
          
          
          </tr>

          <% }) %>
         
      </table>
    </body>
</html>

