</select>
    <body>
        <h1>Casts</h1>
        <%- include('../partials/navbar.ejs') %>
        
        <table border = "2">
            <thead>
                <tr>
                    <th>ID </th>
                    <th>Name</th>
                    <th>Phone Number</th>
                    <th>Birth Year</th>
                    <th>Action</th>
                </tr>
            </thead>

            <tbody>
                <% data.forEach(el=>{ %>
                <tr>
                    <td><%= el.id %></td>
                    <td><%=el.getFullName()%></td>
                    <td><%= el.phone_number %></td>
                    <td><%= el.birth_year %></td>
                    <td>
                    <a href="/cs/<%= el.id %>/edit"><button>Edit</button></a>
                    <a href="/cs/<%= el.id %>/delete"><Button>Delete</Button></a>
                    <a href="/cs/<%= el.id %>/seemovies"><Button>See Movie</Button></a>
                    </td>
                </tr>
                <% }) %>
            </tbody>
        </table>
        <a href='/cs/add'><button>ADD more Cast</button></a>
    </body>



</html>