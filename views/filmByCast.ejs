<%- include('./partials/head')%>

<h1><%= casts.fullname() %></h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Movie</th>
      <th scope="col">Released Year</th>
      <th scope="col">Age When Movie Released</th>
      <th scope="col">Role</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < casts.Movies.length;i++){%>
      <tr>
        <td><%= casts.Movies[i].name %></td>
        <td><%= casts.Movies[i].released_year %></td>
        <td><%= age(casts.Movies[i].released_year, casts.birth_year) %></td>
        <td><%= casts.Movies[i].MovieCast.role %></td>
      </tr>
      <% } %>
  </tbody>
</table>


<%- include('./partials/foot')%>