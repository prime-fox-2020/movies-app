<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('./partials/boostrap.ejs') %>
        <title>Production House</title>
    </head>
    <body>
        <%- include('./partials/navbar.ejs') %>
        <div class="jumbotron">
            <h2 style="padding-bottom: 30px;">List Production Houses</h2>
            <table class="table">
                <thead>
                <tr>
                    <th scope="col">Name Production House</th>
                    <th scope="col">Headquarters</th>
                </tr>
                </thead>
                <% for(let i =0; i < prodHouse.length; i++) { %>
                <tbody>
                <tr>
                    <td><%= prodHouse[i].name_prodHouse %></td>
                    <td><%= prodHouse[i].headquarters %></td>
                </tr>
                </tbody>
                <% } %>
            </table>
        </div>
    </body>
</html>