<%- include('partials/header') %>

<h3><%= data.getFullName() %> </h3>
<table class="table">
    <thead class="thead-dark">
        <tr>
        <th scope="col">Movie</th>
        <th scope="col">Released Year</th>
        <th scope="col">Aged when Movie Released</th>
        <th scope="col">Role</th>
        </tr>
    </thead>
    <tbody>
        <% for( let i = 0; i < data.Movies.length; i++ ) { %>
            <tr>
                <td><%= data.Movies[i].name %></td>
                <td><%= data.Movies[i].released_year %></td>
                <td><%= getAgeWhenReleased(data.birth_year, data.Movies[i].released_year) %></td>
                <td><%= data.Movies[i].MovieCast.role %></td>
                </tr>
        <% } %>
    </tbody>
</table>

<%- include('partials/footer') %>